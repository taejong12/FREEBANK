<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="604.0" maxWidth="600" prefHeight="310.0" prefWidth="400.0" spacing="20" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.exam01.controller.GymController">
	<!-- Title and message -->
	<Label alignment="CENTER" style="-fx-font-size: 24px; -fx-font-weight: bold;" text="GYM Machine Update" />

	<!-- id input -->
	<TextField fx:id="txtType" promptText="Type(등,어깨,하체,가슴)" style="-fx-pref-width: 400px;" />
	<TextField fx:id="txtMachineName" promptText="Machine Name" />
	<TextField fx:id="txtMet" promptText="Met" />
	<TextField fx:id="txtGrowthFactor" promptText="GrowthFactor" />

	<!-- Signin link -->
	<HBox alignment="CENTER">
      <children>
         <Button mnemonicParsing="false" onAction="#logout" prefHeight="34.0" prefWidth="100.0" text="Logout">
            <HBox.margin>
               <Insets left="20.0" right="20.0" />
            </HBox.margin>
         </Button>
      
      
      	<!-- Submit Button -->	
      	<Button onAction="#openManger" style="-fx-pref-width: 100px; -fx-padding: 10px; -fx-background-color: #4CAF50; -fx-text-fill: white;" text="Submit" />
      </children></HBox>
</VBox>
